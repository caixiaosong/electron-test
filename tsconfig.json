    "compilerOptions": {
        "target": "es5",
        "allowJs": true,
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "out",
        "incremental": true
    },
    "include": [
        "src/**/*.ts",
        "src/test-free/**/*.ts"
    ],
    "exclude": [
        "src/test",
        "assert",
        "node_modules",
        "out",
        "dist"
    ]
}